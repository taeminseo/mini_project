<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="admin.main.adminController">
   <children>
      <BorderPane prefHeight="600.0" prefWidth="450.0">
         <top>
            <VBox alignment="CENTER" prefHeight="88.0" prefWidth="450.0" BorderPane.alignment="CENTER">
               <children>
                  <Label prefHeight="47.0" prefWidth="182.0" text="회원 목록">
                     <font>
                        <Font size="40.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
         </top>
         <bottom>
            <VBox prefHeight="97.0" prefWidth="450.0" BorderPane.alignment="CENTER">
               <children>
                  <HBox alignment="CENTER" prefHeight="115.0" prefWidth="450.0" spacing="20.0">
                     <children>
                        <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="110.0" text="뒤로가기" />
                        <Button mnemonicParsing="false" prefHeight="50.0" prefWidth="110.0" text="회원 삭제" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </bottom>
         <center>
            <TableView onMouseClicked="#selecttable" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn prefWidth="75.0" text="회원이름" />
                <TableColumn prefWidth="75.0" text="아이디" />
                  <TableColumn prefWidth="75.0" text="비밀번호" />
                  <TableColumn prefWidth="75.0" text="전화번호" />
                  <TableColumn prefWidth="75.0" text="차번호" />
                  <TableColumn prefWidth="75.0" text="차종" />
              </columns>
            </TableView>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
